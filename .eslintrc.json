{
  "env": {
    "es2021": true,
    "node": true,
    "mocha": true
  },
  "extends": ["airbnb-base"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
  },
  "rules": {
    "import/no-unresolved": "off",
    "import/extensions": "off"
  }
}
